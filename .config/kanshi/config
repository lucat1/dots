profile {
  output eDP-1 enable scale 2
}

# home docking (bologna)
profile {
  output eDP-1 disable
  output "BNQ BenQ GW2470 H8F00853019" enable scale 1
}

# home docking (riccione)
profile {
  output eDP-1 disable
  output "BNQ BenQ GW2470 H8F00852019" enable scale 1 position 0,0
  output "BNQ BenQ GW2470 H8F00853019" enable scale 1 position 1920,0
}

# home docking (zurich)
profile {
