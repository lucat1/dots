pkgname=bars
pkgver=0
pkgrel=0
pkgdesc="*_panel binaries for my lemonbars"
url="none"
arch="all"
license="MIT"
depends=""
depends_dev=""
makedepends=""
install=""
options="!check"
source="Makefile leftbar_panel.c rightbar_panel.c"
